{
    "compilerOptions": {
        "module": "CommonJS",
        "target": "ESNext",
        "moduleResolution": "Node",
        "sourceMap": true,
        "rootDir": "src",
        "outDir": "dist",
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules/**/*"
    ],
}